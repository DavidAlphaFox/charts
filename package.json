{
  "name": "@rsuite/charts",
  "version": "0.0.0-development",
  "description": "ECharts for React",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/esm/index.js"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "format": "prettier --write .",
    "format:check": "prettier --list-different \"{src,test,docs}/**/*.js\"",
    "dev": "dumi dev",
    "docs": "dumi build",
    "build": "tsup src/index.ts --clean --format cjs,esm --legacy-output --dts && npm run build:locales",
    "build:locales": "tsc src/locales/**/*.ts --outDir dist/locales",
    "publish-docs": "node docs/gh-pages.js",
    "prepages": "npm run docs",
    "pages": "npm run publish-docs",
    "prepublishOnly": "npm run build",
    "release": "npm publish --access public --tag next",
    "lint": "eslint .",
    "test": "jest",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "echarts-for-react": "^3.0.2",
    "lodash.merge": "^4.6.2",
    "lodash.omit": "^4.5.0",
    "react-keyed-flatten-children": "^1.2.0"
  },
  "peerDependencies": {
    "echarts": "^5.0.0",
    "react": "^16.8.0 || ^17.0.0"
  },
  "author": "Doma <leishenghao@126.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https:git@github.com:rsuite/charts.git"
  },
  "homepage": "https://github.com/rsuite/charts",
  "devDependencies": {
    "@swc-node/jest": "^1.5.2",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@types/jest": "^26.0.20",
    "@types/lodash.merge": "^4.6.6",
    "@types/lodash.omit": "^4.5.6",
    "@types/react": "^16.9.6",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "cross-env": "^7.0.3",
    "dumi": "^1.1.42",
    "echarts": "^5.0.0",
    "echarts4": "npm:echarts@^4.9.0",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.30.0",
    "gh-pages": "^1.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.7.0",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "semantic-release": "^19.0.2",
    "tsup": "^6.1.2",
    "typescript": "^4.7.3"
  }
}
