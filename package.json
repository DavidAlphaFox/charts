{
  "name": "@rsuite/charts",
  "version": "0.0.0-development",
  "description": "ECharts for React",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/esm/index.js"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "format": "prettier --write \"{src,test,docs}/**/*.js\"",
    "format:check": "prettier --list-different \"{src,test,docs}/**/*.js\"",
    "dev": "dumi dev",
    "docs": "dumi build",
    "build": "tsup src/index.ts --clean --format cjs,esm --legacy-output --dts && npm run build:locales",
    "build:locales": "tsc src/locales/**/*.ts --outDir dist/locales",
    "publish-docs": "node docs/gh-pages.js",
    "prepages": "npm run docs",
    "pages": "npm run publish-docs",
    "prepublishOnly": "npm run build",
    "release": "npm publish --access public --tag next",
    "lint": "eslint .",
    "test": "jest",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "echarts-for-react": "^3.0.2",
    "lodash.merge": "^4.6.2",
    "lodash.omit": "^4.5.0",
    "react-keyed-flatten-children": "^1.2.0"
  },
  "peerDependencies": {
    "echarts": "^5.0.0",
    "react": "^16.8.0 || ^17.0.0"
  },
  "author": "Doma <leishenghao@126.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https:git@github.com:rsuite/charts.git"
  },
  "homepage": "https://github.com/rsuite/charts",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
